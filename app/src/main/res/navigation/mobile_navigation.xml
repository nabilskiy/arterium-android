<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">


    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.maritech.arterium.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_statistics"
        android:name="com.maritech.arterium.ui.statistics.StatFragment"
        android:label="@string/title_statistics"
        tools:layout="@layout/fragment_stat" />
    <fragment
        android:id="@+id/navigation_achives"
        android:name="com.maritech.arterium.ui.achieves.AchievesFragment"
        android:label="@string/title_achives"
        tools:layout="@layout/fragment_achives" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.maritech.arterium.ui.profile.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.maritech.arterium.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>