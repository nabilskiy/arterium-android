<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomProgressBar">
        <attr name="cp_value" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomInput">
        <attr name="ci_title" format="string"/>
        <attr name="ci_field_value" format="string"/>
    </declare-styleable>

    <declare-styleable name="OrangeProgressBar">
        <attr name="opb_value" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomTabComponent">
        <attr name="csc_count" format="integer" />
        <attr name="csc_text_one" format="string" />
        <attr name="csc_text_two" format="string" />
        <attr name="csc_text_three" format="string" />
        <attr name="csc_text_four" format="string" />
    </declare-styleable>

    <declare-styleable name="CircularProgress">
        <attr format="float" name="max"/>
        <attr format="float" name="progress"/>
        <attr format="dimension" name="backgroundRingSize"/>
        <attr format="dimension" name="progressRingSize"/>
        <attr format="color|reference" name="backgroundRingColor"/>
        <attr format="color|reference" name="progressRingColor"/>
        <attr format="reference" name="progressGradient"/>
        <attr format="boolean" name="joinGradient"/>
        <attr format="float" name="gradientFactor"/>
        <attr format="boolean" name="progressRingOutline"/>
        <attr name="progressRingCorner">
            <enum name="ROUND" value="1"/>
        </attr>
    </declare-styleable>
</resources>